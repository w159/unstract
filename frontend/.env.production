# Production environment variables for frontend
# These get baked into the build at compile time

# Backend API URL - should use relative paths in production
REACT_APP_BACKEND_URL=

# Feature flags
REACT_APP_FEATURE_FLAGS_ENABLED=true

# Analytics (optional)
REACT_APP_POSTHOG_KEY=
REACT_APP_POSTHOG_HOST=

# Sentry error tracking (optional)
REACT_APP_SENTRY_DSN=

# Custom branding (optional)
REACT_APP_CUSTOM_LOGO_URL=
REACT_APP_FAVICON_PATH=

# Environment
REACT_APP_ENV=production

# Disable source maps in production
GENERATE_SOURCEMAP=false